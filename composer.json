{
    "name": "keizai-tech/ip-tools",
    "description": "IP Tools Suite - Geolocation tracking and analytics platform",
    "type": "project",
    "version": "2.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Keizai Tech",
            "email": "contact@keizai-tech.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "mkdir -p logs",
            "chmod 755 logs"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 